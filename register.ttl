bblocks:r1.geo.common.data_types.bounding_box a bblocks:Schema ;
    rdfs:label "Bounding Box" ;
    bblocks:hasSchema <https://opengeospatial.github.io/bblocks/annotated-schemas/geo/common/data_types/bounding_box/schema.json>,
        <https://opengeospatial.github.io/bblocks/annotated-schemas/geo/common/data_types/bounding_box/schema.yaml> ;
    bblocks:inRegister bblocks:ogc-building-block-register ;
    bblocks:maturity <http://example.com/ogc/def/bblocks/maturity/mature> ;
    bblocks:scope <http://example.com/ogc/def/bblocks/scope/geo> ;
    bblocks:status <http://www.opengis.net/def/status/valid> ;
    dct:created "2022-05-24T13:51:38+00:00"^^xsd:dateTime ;
    dct:description "The bounding box JSON object describes a simple spatial extent of a resource. For OGC APIâ€™s this could be a feature, a feature collection or a dataset, but it can be used in any JSON resource that wants to communicate its rough location. The extent is <i>simple</i> in that the bounding box does not describe the precise location and shape of the resource, but provides an axis-aligned approximation of the spatial extent that can be used as an initial test whether two resources are potentially intersecting each other." ;
    dct:hasVersion "1.0.1" ;
    dct:modified "2023-03-09"^^xsd:date ;
    dct:source [ rdfs:label "OGC API - Features, Part 1, 7.13.2: Feature Collections Response" ;
            rdfs:seeAlso <http://www.opengis.net/doc/IS/ogcapi-features-1/1.0#_response_4> ] ;
    rdfs:seeAlso <https://opengeospatial.github.io/bblocks/generateddocs/markdown/geo/common/data_types/bounding_box/index.md> .

bblocks:r1.geo.common.data_types.ogc_collection a bblocks:Schema ;
